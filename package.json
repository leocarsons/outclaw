{
  "name": "outclaw",
  "version": "0.2.3",
  "description": "CLI tool for managing OpenClaw Skills - Install, publish and manage skills from Outclaws registry",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "outclaw": "./dist/index.js",
    "ocl": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "skills",
    "cli",
    "registry",
    "ai",
    "agent",
    "agent-skills"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/outclaw.git"
  },
  "homepage": "https://github.com/yourusername/outclaw#readme",
  "bugs": {
    "url": "https://github.com/yourusername/outclaw/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "gray-matter": "^4.0.3",
    "ora": "^8.1.1",
    "semver": "^7.6.3",
    "simple-git": "^3.27.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/semver": "^7.5.8",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
